  <license>AGPL-3.0</license>
  <maintainer>
    <author>Tobias Lounsbury</author>
    <email>toby@ginkgostreet.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/ginkgostreet/com.ginkgostreet.securefiles</url>
    <url desc="Documentation">https://github.com/ginkgostreet/com.ginkgostreet.securefiles</url>
    <url desc="Support">https://github.com/ginkgostreet/com.ginkgostreet.securefiles</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2015-12-28</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>4.6</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <civix>
    <namespace>CRM/Securefiles</namespace>
  </civix>
</extension>
